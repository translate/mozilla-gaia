msgstr "OK"

#: download-button
msgid "Download"
msgstr "Yuklab olish"

#: install-button
msgid "Install"
msgstr "O‘rnatish"

#: cancel-button
msgid "Cancel"
msgstr "Bekor qilish"

#: download_error
msgid "Download Error"
msgstr "Yuklab olishda xatolik"

#: install_error
msgid "Installation Error"
msgstr "O‘rnatishda xatolik"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "Yuklab olish server xatoligi tufayli amalga oshmadi."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "Yuklab olish server xatoligi tufayli amalga oshmadi."

#: err_too_big
msgid "File is too large to download."
msgstr "Fayl yuklab olish uchun juda katta."

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "Siz xarid qilgan media turini telefoningizda ishga tushirib bo‘lmaydi."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "Yuklab olish server xatoligi tufayli amalga oshmadi."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "Yuklab olish server xatoligi tufayli amalga oshmadi."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "Yuklab olish server xatoligi tufayli amalga oshmadi."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "Yuklab olingan rasmni ushbu qurilmadada ko‘rsatib bo‘lmaydi."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "Yuklab olingan faylni ushbu qurilmada ijro qilib bo‘lmaydi."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr "Ushbu qo‘shiqni saqlash uchun qurilmangizda yetarlicha bo‘sh joy yo‘q."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "Xotira kartasi bo‘lmaganligi uchun qo‘shiqni saqlab bo‘lmaydi."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Xotira kartasidan foydalanayotganligi uchun qo‘shiqni saqlab bo‘lmaydi."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Media faylni saqlashda xatolik."

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Media faylni saqlashda xatolik."

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr ""
"Qo‘shiq yuklab olindi va o‘rnatildi. Uni eshitish uchun \"Musiqa\" ilova "
"dasturidan foydalaning."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "Rington yuklab olindi va o‘rnatildi."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "Fon rasmi yuklab olindi va o‘rnatildi."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr ""
"Ushbu audio faylni qo‘shiq yoki rington sifatida o‘rnatishni xohlaysizmi?"

#: ringtone-response
msgid "Ringtone"
msgstr "Rington"

#: song-response
msgid "Song"
msgstr "Qo‘shiq"

#: download-confirmation-title-song
msgid "Download song?"
msgstr "Qo‘shiq yuklab olinsinmi?"

#: download-confirmation-title-ringtone
msgid "Download ringtone?"
msgstr "Rington yuklab olinsinmi?"

#: download-confirmation-title-wallpaper
msgid "Download wallpaper?"
msgstr "Fon rasmi yuklab olinsinmi?"

