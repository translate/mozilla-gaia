#. # create-collection
#: loading
msgid "Loading"
msgstr "កំពុង​ផ្ទុក"

#: cancel
msgid "Cancel"
msgstr "បោះបង់"

#: custom
msgid "Custom"
msgstr "ផ្ទាល់ខ្លួន"

#: network-error-message
msgid "Connect to the internet to add more Smart Collections"
msgstr "ភ្ជាប់​អ៊ីនធឺណិត​ ដើម្បី​បន្ថែម​សម្រាំង​វៃ​ឆ្លាត​បន្ថែម"

#: no-available-collections
msgid "All smart collections have been added."
msgstr "ការ​ប្រមូល​ដ៏​ឆ្លាតវៃ​ត្រូវ​បាន​បន្ថែម​។"

#. # view-collection
#: add-to-top-of-collection
msgid "Add to top of collection"
msgstr "បន្ថែម​សម្រាំង​ផ្នែក​ខាងលើ"

#: add-to-home-screen
msgid "Add to Home Screen"
msgstr "បន្ថែម​ទៅ​អេក្រង់​ដើម"

#: offline-webresults
msgid "Connect to the internet to get apps for {{collectionName}}"
msgstr "ភ្ជាប់​អ៊ីនធឺណិត​ដើម្បីទទួល​កម្មវិធី​សម្រាប់ {{collectionName}}"

#: edit-mode
msgid "Edit"
msgstr "កែសម្រួល​"

#: edit-done
msgctxt "edit-done"
msgid "Done"
msgstr "ធ្វើ​រួច"

#. # delete-collection
#: delete-generic-title
msgid "Delete Collection"
msgstr "លុប​ការប្រមូល​"

#: delete-generic-body
msgid "This collection and all of its data will be deleted."
msgstr "ការប្រមូល​នេះ​ និង​ទិន្នន័យ​វា​ទាំងអស់​នឹង​ត្រូវបាន​លុបចោល​។​"

#: delete
msgid "Delete"
msgstr "លុប"

#. # update-collection
#: edit-collection-header
msgid "Edit collection"
msgstr "កែ​ការ​ប្រមូលផ្ដុំ"

#: done
msgctxt "done"
msgid "Done"
msgstr "ធ្វើ​រួច"

