#. # create-collection
#: loading
msgid "Loading"
msgstr "កំពុង​ផ្ទុក"

#: cancel
msgid "Cancel"
msgstr "បោះបង់"

#: custom
msgid "Custom"
msgstr "ផ្ទាល់ខ្លួន"

#: prompt-create-custom
msgid "Create your own Smart Collection"
msgstr "បង្កើត​សម្រាំង​ដ៏​វៃ​ឆ្លាត​ផ្ទាល់ខ្លួន​របស់​អ្នក"

#: network-error-message
msgid "Connect to the internet to add more Smart Collections"
msgstr "ភ្ជាប់​អ៊ីនធឺណិត​ ដើម្បី​បន្ថែម​សម្រាំង​វៃ​ឆ្លាត​បន្ថែម"

#. # view-collection
#: add-to-top-of-collection
msgid "Add to top of collection"
msgstr "បន្ថែម​សម្រាំង​ផ្នែក​ខាងលើ"

#: add-to-home-screen
msgid "Add to Home Screen"
msgstr "បន្ថែម​ទៅ​អេក្រង់​ដើម"

#: offline-webresults
msgid "Connect to the internet to get apps for {{collectionName}}"
msgstr "ភ្ជាប់​អ៊ីនធឺណិត​ដើម្បីទទួល​កម្មវិធី​សម្រាប់ {{collectionName}}"

#: edit-mode
msgid "Edit"
msgstr "កែសម្រួល​"

#: edit-done
msgctxt "edit-done"
msgid "Done"
msgstr "ធ្វើ​រួច"

#. # delete-collection
#: delete-title
msgid "Delete {{name}}"
msgstr "លុប {{name}}"

#: delete-body
msgid "{{name}} and all of its data will be deleted."
msgstr "នឹង​លុប {{name}} និង​ទិន្នន័យ​របស់​វា​ទាំងអស់។"

#: delete
msgid "Delete"
msgstr "លុប"

#. # update-collection
#: edit-collection-header
msgid "Edit collection"
msgstr "កែ​ការ​ប្រមូលផ្ដុំ"

#: done
msgctxt "done"
msgid "Done"
msgstr "ធ្វើ​រួច"
