#: voiceMail
msgid "Voicemail"
msgstr "សារ​ជា​សំឡេង"

#~ msgid "To make a call you need to disable airplane mode in settings."
#~ msgstr "ដើម្បី​ហៅ​ចេញ​អ្នក​ត្រូវ​បិទ​របៀប​លើ​យន្តហោះ​នៅ​ក្នុង​ការ​កំណត់។"

#~ msgctxt "callAirplaneModeBtnOk"
#~ msgid "OK"
#~ msgstr "យល់​ព្រម"

#~ msgid "Airplane mode activated"
#~ msgstr "ធ្វើ​ឲ្យ​សកម្ម​របៀប​លើ​យន្តហោះ"

#~ msgid "Number is busy"
#~ msgstr "លេខ​ជាប់​រវល់"

#~ msgid "The number is currently busy. Please try again later."
#~ msgstr "បច្ចុប្បន្ន​លេខ​កំពុង​ជាប់​រវល់។ សូម​ព្យាយាម​ម្ដង​ទៀត​ពេល​ក្រោយ។"

#~ msgid "FDN active"
#~ msgstr "FDN សកម្ម"

#~ msgid ""
#~ "FDN (Fixed Dialing Numbers) is activated and the following number is not "
#~ "in your FDN list: {{ number }}"
#~ msgstr ""
#~ "FDN (Fixed Dialing Numbers) ត្រូវ​បាន​ធ្វើ​ឲ្យ​សកម្ម និង​លេខ​បន្ទាប់​មិន​មាន​នៅ​ក្នុង​បញ្ជី FDN "
#~ "របស់​អ្នក​ឡើយ៖ {{ number }}"

#~ msgid "Emergency number"
#~ msgstr "លេខ​ហៅ​បន្ទាន់"

#~ msgid "To make a call, the phone must be connected to a network."
#~ msgstr "ដើម្បី​​ធ្វើ​ការ​ហៅ ទូរស័ព្ទ​ត្រូវតែ​ភ្ជាប់​ទៅ​បណ្ដាញ។"

#~ msgid "Emergency calls are not allowed by the network."
#~ msgstr "មិន​អនុញ្ញាត​ការ​ហៅ​បន្ទាន់​តាម​បណ្ដាញ​នេះ​ទេ។"

#~ msgctxt "emergencyDialogBtnOk"
#~ msgid "OK"
#~ msgstr "យល់​ព្រម"

#~ msgid "No network connection"
#~ msgstr "គ្មាន​ការ​តភ្ជាប់​បណ្ដាញ"

#~ msgid "Invalid phone number"
#~ msgstr "លេខ​ទូរស័ព្ទ​មិន​ត្រឹមត្រូវ"

#~ msgid "The phone number you are calling is not valid."
#~ msgstr "លេខ​ទូរស័ព្ទ​ដែល​អ្នក​កំពុង​ហៅ​មិន​ត្រឹមត្រូវ​ទេ។"

#~ msgid "Unable to make a phone call now"
#~ msgstr "មិន​អាច​ធ្វើ​ការ​ហៅ​ទូរស័ព្ទ​ឥឡូវ​នេះ​បាន​ទេ"

#~ msgid ""
#~ "Cannot make a call. If you are already dialing, please hang up first."
#~ msgstr "មិន​អាច​ធ្វើ​ការ​ហៅ។ បើ​អ្នក​​​បាន​ចុច​លេខ​រួច​ហើយ សូម​ដាក់​ទូរស័ព្ទ​​ចុះ​សិន។"

#~ msgid "Other connection in use"
#~ msgstr "ការ​តភ្ជាប់​ផ្សេង​ទៀត​កំពុង​ប្រើ"

#~ msgid ""
#~ "You have an active call on another connection. You can only use one "
#~ "connection at a time."
#~ msgstr ""
#~ "អ្នក​មាន​ការ​ហៅ​សកម្មភាព​នៅ​លើ​ការ​ហៅ​ផ្សេង​ទៀត។ អ្នក​អាច​ប្រើ​​តែ​ការ​តភ្ជាប់​មួយ​ប៉ុណ្ណោះ​នៅ​​ពេល​តភ្ជាប់​"
#~ "ពេល​តែមួយ។"

#~ msgid ""
#~ "This number is blocked as it is not in your list of authorized numbers."
#~ msgstr "លេខ​នេះ​ត្រូវ​បានទប់ស្កាត់ ព្រោះ​វា​មិន​នៅ​ក្នុង​បញ្ជី​​លេខ​ដែល​បានអនុញ្ញាត​របស់​អ្នក។"
