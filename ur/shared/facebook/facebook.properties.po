#. # Message
#: message2
msgctxt "message2"
msgid "This may take a couple of minutes"
msgstr "یہ کچھ منٹ لے سکتا ہے"

#: messageLinking
msgid "Please hold on. We are linking your friend and your contact."
msgstr "براہ مہربانی رک جائے۔ ہم آپ کے دوست اور آپ کی رابطہ منسلک کر رہے ہیں۔"

#: messageCanceling
msgid "Please hold on. We are canceling the ongoing operation."
msgstr "براہ مہربانی انتظار کریں۔ ہم جاری عملیہ منسوخ کر رہے ہیں۔"

#. # Errors
#: error1
msgid "Error {{from}}"
msgstr "خرابی {{from}}"

#: error2
msgctxt "error2"
msgid "Do you want to retry?"
msgstr "کیا آپ چاہتے ہیں کہ پھر کوشش کریں؟"

#: errorProposal
msgctxt "errorProposal"
msgid "calculating proposals"
msgstr "تجاویز کا حساب لگا یا جارہا ہے"

#: errorFriends
msgctxt "errorFriends"
msgid "retrieving friends"
msgstr "دوست بازگیر کر رہا ہے"

#: errorLinking
msgid "linking contact"
msgstr "کونٹیکٹس کو لنک کیا جارہا ہے"

#. # Timeouts
#: timeout1
msgid "Exceeded timeout {{from}}"
msgstr "تجاوز کردہ وقت {{from}}"

#: timeout2
msgctxt "timeout2"
msgid "Do you want to retry?"
msgstr "کیا آپ چاہتے ہیں کہ پھر کوشش کریں؟"

#: timeoutProposal
msgctxt "timeoutProposal"
msgid "calculating proposals"
msgstr "تجاویز کا حساب لگا یا جا رہا ہے"

#: timeoutFriends
msgctxt "timeoutFriends"
msgid "retrieving friends"
msgstr "دوست بازگیر کر رہا ہے"

#: timeoutLinking
msgid "linking contacts"
msgstr "رابطہ افراد ربط کر رہا ہے"

#. # Wait
#: wait1
msgid "Please hold on"
msgstr "براہ مہربانی پر منعقد کریں"

#: wait2
msgctxt "wait2"
msgid "This may take a couple of minutes"
msgstr "اس میں چند منٹ لگ سکتے ہیں"

#: waitProposal
msgid "Please hold on. We are calculating a proposal."
msgstr "براہ مہربانی رک جائے۔ ہم نے ایک تجویز حساب کر رہے ہیں۔"

#: waitFriends
msgid "Please hold on. We are connecting to obtain your friend list."
msgstr ""
"براہ مہربانی رک جائے۔ ہم نے اپنے دوست کی فہرست حاصل کرنے کے لئے منسلک کر رہے "
"ہیں۔"

#: curtainCancel
msgctxt "curtainCancel"
msgid "Cancel"
msgstr "منسوخ کریں"

#: curtainRetry
msgid "Retry"
msgstr "پھر کوشش کریں"

#: curtainOK
msgctxt "curtainOK"
msgid "OK"
msgstr "ٹھیک ہے"

#: connectionLost
msgid "No Internet connection"
msgstr "کوئی انٹرنیٹ کنکشن نہیں"

#: connectionLostMsg
msgid "Friends’ photos will not be added to your contacts. Continue anyway?"
msgstr ""
"دوستوں کی تصاویر آپ کے رابطہ افراد میں نہیں آئیں گی۔ پھر بھی جاری رکھیں؟"

#: yesOption
msgctxt "yesOption"
msgid "OK"
msgstr "ٹھیک ہے"

#: noOption
msgctxt "noOption"
msgid "Cancel"
msgstr "منسوخ کریں"

