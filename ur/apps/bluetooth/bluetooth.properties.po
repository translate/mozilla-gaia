#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "بلوٹوتھ"

#: confirmation
msgid "Confirmation"
msgstr "تصدیق "

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "بلوٹوتھ کو غیر فعال کر دیا ہے"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "کیا آپ بلوٹوت کو چالو کرنا چاہتے ہیں؟"

#: cancel
msgid "Cancel"
msgstr "منسوخ کریں"

#: turn-on
msgid "Turn On"
msgstr "چالو کریں"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "بلوٹوتھ فائل ٹرانسفر ناکام ہوگئی"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"بلوٹوتھ فائل ٹرانسفر ناکام ہوگئی ہے. چیک کریں کہ بلوٹوت کی ترتیبات درست ہیں۔"

#: ok
msgid "OK"
msgstr "ٹھیک ہے"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "جوڑی شدہ آلے"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "اس علاقہ میں آلات"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "رابطہ قائم کرنے کے لئے تھپتھپائیں"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "آلے کے ساتھ جوڑی بنا رہا ہے…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "دوسرے آلے کا انتظار کر رہا ہے…"

#: device-status-paired
msgid "Paired"
msgstr "جوڑا شدہ"

#: device-status-connecting
msgid "Connecting…"
msgstr "جڑ رہا ہے…"

#: device-status-connected
msgid "Connected"
msgstr "كنیكٹ کر گیا ہے"

#: search-for-device
msgid "Searching for devices…"
msgstr "آلات کی تلاش کر رہا ہے…"

#: search-device
msgid "Search for devices"
msgstr "آلات تلاش کریں"

#: unnamed-device
msgid "Unknown device"
msgstr "نامعلوم آلہ"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "آلات جوڑنے میں ناکام"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "آلہ سے جڑنے میں ناکام۔ پڑتال کریں کہ PIN صحیح ہے۔"

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "ڈیوائس سے جڑنے میں ناکام۔ بہت زیادہ درخواست ہیں۔"

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "جوڑا"

#: active-pair
msgid "Confirm devices to pair"
msgstr "آلات کو جوڑنے کی تصدیق کریں"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"{{device}} سے جڈنے کے لے، اثبات کریں کی دونو ڈوایس پر مندرجہ ذیل PIN دکھ رoہ "
"ہو:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "{{device}} سے جڑنے کے لے، آلے پر PIN داخل کریں۔"

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "{{device}} سے جڈنے کے لئے، ڈیوائس میں پاسس کوڈ داخل کاریں۔"

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "بلوٹوتھ جوڑنے کی فرمائش"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} اس فون سے جڑنا چاہتا ہے۔ .قبول کرنے کے لئے, پککہ کریں کی دونو "
"ڈیوائس پر مندرجہ ذیل PIN دکھایَ گےَ ہے:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} اس فون سے جڑنا چاہتا ہے۔ قبول کرنے کے لیے، اس ڈیوائس میں اپنا PIN "
"داخل کرئیں۔"

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} اس فون سے جڑنا چاہتا ہے۔ قبول کرنے کے لیے، اس ڈیوائس میں اپنا پاس "
"کی داخل کرئیں۔"

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "بلوٹوت کی درخواست لیے -"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "بلوٹوت جوڑ نے کی درخواست -"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "بلوٹوتھ جوڈی جوڑنے کی درخواست"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "بلوٹوت جوڑنے کی درخواست ختم ہوئی۔ پھر سے جوڑنے کی کوشش کریں۔"

