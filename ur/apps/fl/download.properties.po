#: ok
msgid "OK"
msgstr "ٹھیک ہے"

#: download-button
msgid "Download"
msgstr "ڈاؤن لوڈ"

#: install-button
msgid "Install"
msgstr "تنصیب کریں"

#: cancel-button
msgid "Cancel"
msgstr "منسوخ کریں"

#: download_error
msgid "Download Error"
msgstr "ڈاؤن لوڈ نقص"

#: install_error
msgid "Installation Error"
msgstr "تنصیب کرنے میں نقص آگیا"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "سرور کی خرابی کی وجہ سے ڈاؤن لوڈ ناکام ہوا۔"

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "سرور کی خرابی کی وجہ سے ڈاؤن لوڈ ناکام ہوا۔"

#: err_too_big
msgid "File is too large to download."
msgstr "مسل ڈاؤن لوڈ کرنے کے لئے بہت بڑی ہے۔"

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "اس قسم کا خریدا حوا میڈیا آپ کے فون پر معاونت شدہ نہیں ہے۔"

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "سرور کی خرابی کی وجہ سے ڈاؤن لوڈ ناکام ہوا۔"

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "سرور کی خرابی کی وجہ سے ڈاؤن لوڈ ناکام ہوا۔"

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "سرور کی خرابی کی وجہ سے ڈاؤن لوڈ ناکام حوا۔"

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "ڈاؤن لوڈ کی حوی تصویر اس آلہ پر ڈسپلے نہیں کی جا سکتی۔"

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "ڈاؤن لوڈ کی حوی تصویر اس آلہ پر ڈسپلے نہیں کی جا سکتی۔"

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr "اس گیت کو محفوظ کرنے کے لئے آپ کے آلے پر کافیا سٹوریج کی جگہ نہیں ہے۔"

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "گانا محفوظ نہیں کر سکتا کیونکہ کوئی میموری کارڈ نہیں ہے۔"

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"گانا محفوظ نہیں کر سکتا کیونکہ میموری کارڈ استعمال میں ہے۔ اپنا فون نکال کر "
"پھر ڈاؤن لوڈ کرنے کی کوشش کریں۔"

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "میڈیا مسل کو محفوظ کرنے میں نقص۔"

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "میڈیا مسل کو محفوظ کرنے میں نقص۔"

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr ""
"گانا ڈائونلوڈ اور نصب ہوگیا ہے۔ چلانے کے لیئے موسیقی کی ایپلیکیشن استعمال "
"کریں۔"

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "گھنٹی کی آواز ڈائونلوڈ اور نصب ہوگئی ہے۔"

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "والپیپر ڈائونلوڈ اور نصب ہوگیا ہے۔"

#: ringtone-or-song-query
msgid ""
"Do you want to download and install this audio file as a song or as a "
"ringtone?"
msgstr ""
"آپ اس سمعی فائل کو گانے کی صورت میں یا گھنٹی کی آواز کی صورت میں ڈاؤن لوڈ "
"اور تنصیب کرنا چاہتے ہیں؟"

#: ringtone-response
msgid "Ringtone"
msgstr "گھنٹی کی آواز"

#: song-response
msgid "Song"
msgstr "گانا"

#: download-confirmation-title-song
msgid "Download song?"
msgstr "گانا ڈاؤن لوڈ کریں؟"

#: download-confirmation-title-ringtone
msgid "Download ringtone?"
msgstr "گھنٹی کی آواز ڈائونلوڈ کریں؟"

#: download-confirmation-title-wallpaper
msgid "Download wallpaper?"
msgstr "وال پیپر ڈاؤن لوڈ کریں؟"

#: downloading-song
msgid "Downloading song…"
msgstr "گانا ڈاؤن لوڈ کر رہا ہے…"

#: downloading-ringtone
msgid "Downloading ringtone…"
msgstr "گھنٹی کی آواز ڈاؤن لوڈ ہو رہی ہے…"

#: downloading-wallpaper
msgid "Downloading wallpaper…"
msgstr "وال پیپر ڈاؤن لوڈ کر رہا ہے…"

#: locked-content-advisory
msgid ""
"{{vendor}} has locked this media file so that it cannot be shared with "
"others or transferred to a new device."
msgstr ""
"{{vendor}} نے اس میڈیا فائل کو مقفل کردیا ہے تاکہ کوئی اس کا حصہ دار نہ بن "
"سکے یا اس کو نئے آلہ میں منتقل نہ کرسکے۔"

#. # This version is like the above, but used when we don't know the name of
#. # the vendor.
#: locked-content-advisory-generic
msgid ""
"The seller has locked this media file so that it cannot be shared with "
"others or transferred to a new device."
msgstr ""
"بیچنے والے نے اس میڈیا فائل کو مقفل کردیا ہے تاکہ کوئی اس کا حصہ دار نہ بن "
"سکے یا اس کو نئے آلہ میں منتقل نہ کرسکے۔"

#: download-confirmation-name
msgid "Name"
msgstr "نام"

#: download-confirmation-type
msgid "Type"
msgstr "قسم"

#: download-confirmation-size
msgid "Size"
msgstr "سائز"

#: download-confirmation-vendor
msgid "Vendor"
msgstr "فروخت کار"

#: download-confirmation-description
msgid "Info"
msgstr "معلومات"

#: download-progress-title
msgid "Downloading"
msgstr "ڈاؤن لوڈ کر رہا ہے"

#: preview-image-title
msgid "Preview image"
msgstr "نقش پیش نظارہ"

#: preview-audio-title
msgid "Preview audio"
msgstr "سمعی پیش نظارہ"

#: byteUnit-B
msgid "bytes"
msgstr "بائٹس"

#: byteUnit-KB
msgid "KB"
msgstr "کلو بائٹ"

#: byteUnit-MB
msgid "MB"
msgstr "میگا بائٹ"

#: byteUnit-GB
