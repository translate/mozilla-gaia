#: manage-title-2
msgid "Manage Tones"
msgstr "ٹون کا انتظام کریں"

#: actions-share-ringtone
msgid "Share ringtone"
msgstr "گھنٹی کی آواز کو شیئر کریں"

#: actions-delete-ringtone
msgid "Delete ringtone"
msgstr "رنگ ٹون کو حذف کریں"

#: actions-share-alerttone
msgid "Share alert"
msgstr "شیئر انتباہ"

#: actions-delete-alerttone
msgid "Delete alert"
msgstr "انتباہ حذف کریں"

#: actions-cancel
msgctxt "actions-cancel"
msgid "Cancel"
msgstr "منسوخ کریں"

#: delete-title-ringtone
msgid "Delete Ringtone"
msgstr "رنگ ٹون حذف کریں"

#: delete-title-alerttone
msgid "Delete Alert"
msgstr "انتباہ حذف کریں"

#: delete-message
msgid "Are you sure you want to delete \"<bdi>{{tone}}</bdi>\"?"
msgstr "آپ کو یقین ہے کے آپ \"<bdi>{{tone}}</bdi>\" کو حذف کرنا چاہتے ہیں؟"

#: delete-message-default-ringtone
msgid ""
"\"<bdi>{{tone}}</bdi>\" is the current default ringtone.<br><br>Are you sure "
"you want to delete it?"
msgstr ""
"\"<bdi>{{tone}}</bdi>\" موجودہ طےشدہ رنگ ٹون ہے۔ <br><br>کیا آپ اس کو واقعی "
"حذف کرنا چاہتے ہیں؟"

#: delete-message-default-alerttone
msgid ""
"\"<bdi>{{tone}}</bdi>\" is the current default alert.<br><br>Are you sure "
"you want to delete it?"
msgstr ""
"\"<bdi>{{tone}}</bdi>\" موجودہ طےشدہ انتباہ ہے۔ <br><br>کیا آپ اس کو واقعی "
"حذف کرنا چاہتے ہیں؟"

#: delete-cancel
msgctxt "delete-cancel"
msgid "Cancel"
msgstr "منسوخ کریں"

#: delete-confirm
msgid "Delete"
msgstr "حذف کریں"

#: deleted-ringtone
msgid "Ringtone deleted."
msgstr "رنگ ٹون حذف کردہ۔"

#: deleted-alerttone
msgid "Alert deleted."
msgstr "انتباہ حذف کردہ۔"

#: created-tone
msgid "New ringtone created."
msgstr "نیا رینگ ٹھونس بنا ہوا۔"

#: set-default-tone
msgid "Ringtone set as default."
msgstr "رینگ ٹون کو بطور طے شدہ سیٹ کریں۔"

#: sd-card-subtitle
msgid "SD Card"
msgstr "ایسڈی کارڈ"

#. ##### Ringtone Picking #####
#: pick-title
msgid "Select Sound"
msgstr "آواز منتخب کریں"

#: pick-set
msgid "Set"
msgstr "سیٹ"

#. ##### List Section Headers #####
#: section-title-builtin-ringtone
msgid "{{ringtoneSectionBrandName}} Ringtones"
msgstr "{{ringtoneSectionBrandName}} گھنٹی کی آواز"

#: section-title-builtin-alerttone
msgid "{{ringtoneSectionBrandName}} Alerts"
msgstr "{{ringtoneSectionBrandName}} انتباہ"

#: section-title-custom-ringtone
msgid "My Ringtones"
msgstr "میری گھنٹی کی آوازیں"

#: section-title-custom-alerttone
msgid "My Alerts"
msgstr "میری انتباہ"

#. ##### Ringtone Sharing (creation) #####
#: create-title
msgid "Create Ringtone"
msgstr "رنگ ٹون بنائیں"

#: create-save
msgid "Save"
msgstr "محفوظ کریں"

#: create-cancel
msgctxt "create-cancel"
msgid "Cancel"
msgstr "منسوخ کریں"

#: play-error-title
msgid "Corrupted File"
msgstr "خراب شدہ مسل"

#: play-error-desc
msgid "The file is corrupted. It cannot be used to create a ringtone."
msgstr "مسل خراب شدہ ہے۔ یہ رنگ ٹوں بنانے کے استمعال میں نہیں اسکتا ہے۔"

#: save-error-title
msgid "Ringtone Not Created"
msgstr "رنگ ٹون نہیں بن سکی"

#: save-error-desc
msgid "There was an issue creating the ringtone. Please try again."
msgstr "رنگ تون بنانے میں ایک ایشو ہے۔ پھر سے کوشش کریں۔"

#: default-label
msgid "Use as default ringtone"
msgstr "طے شدہ رنگ ٹون استعمال کریں"

#: ok
msgid "OK"
msgstr "ٹھیک ہے"

#: ringtone-untitled
msgid "Untitled"
msgstr "بلاعنوان"

