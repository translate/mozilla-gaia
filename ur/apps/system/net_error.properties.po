#: network-error-in-app
msgid "No access to the Internet right now."
msgstr "اس وکت انٹرنیٹ تک رسائی نہیں ہے۔"

#: network-error-launching
msgid "{{name}} requires an Internet connection."
msgstr "{{name}} انٹرنیٹ کنکشن کی ضرورت ہے۔"

#: server-not-found
msgid "Server not found"
msgstr "پیش کار نہیں ملا"

#: server-not-found-error
msgid "Can’t find the server at {{name}}."
msgstr "{{name}} پر پیش کار نہیں مل سکا۔"

#: file-not-found
msgid "File not found"
msgstr "مسل نہیں ملی"

#: file-not-found-error
msgid "The device can’t display this page because the file cannot be found."
msgstr "آلہ اس صفحے کو دکھا نہیں سکتا ہیں کیونکہ مسل نہیں ملی۔"

#: connection-failed
msgctxt "connection-failed"
msgid "Unable to connect"
msgstr "جڑنے میں ناکام"

#: connection-failed-error
msgctxt "connection-failed-error"
msgid "A network error occurred while trying to reach the site."
msgstr "سائٹ تک پہنچنے کی کوشش کے دوران ایک نیٹ ورک خامی پیش آگئی۔"

#: invalid-connection
msgctxt "invalid-connection"
msgid "Unable to connect"
msgstr "جڑنے میں ناکام"

#: invalid-connection-error
msgctxt "invalid-connection-error"
msgid "A network error occurred while trying to reach the site."
msgstr "سائٹ تک پہنچنے کی کوشش کے دوران ایک نیٹ ورک خامی پیش آگئی۔"

#: malformed-uri
msgid "URL not valid"
msgstr "URL جائز نہیں"

#: malformed-uri-error
msgid "The URL entered is not valid and cannot be loaded."
msgstr "داخل کیا ہوا URL جائز نہیں ہے اور لوڈ نہیں کیا جا سکتا۔"

#: redirect-loop
msgctxt "redirect-loop"
msgid "Can’t display page"
msgstr "صفحے کو نہیں دکھا سکتے"

#: redirect-loop-error
msgid ""
"The device can’t display this page because the page is not redirecting "
"properly. Try this page again at a later time."
msgstr ""
"آلہ اس صفحے کو ظاہر نہیں کر سکتا کیونکہ صفحہ صحیع طریقے سے دوسری سمت نہیں "
"بھیج پا رہا ہے۔ بعد میں کسی وقت ایک بار پھر اس صفحے کوشش کریں۔"

#: not-cached
msgid "Page expired"
msgstr "صفحہ زائدالمدت"

#: not-cached-error
msgid ""
"As a security precaution, this page cannot be displayed by the device "
"because it has expired."
msgstr ""
"سیکورٹی احتیاط کے طور پر، آلہ اس صفحے کو ظاہر نہیں کر سکتا کیونکہ یہ صفحہ "
"زائدالمدت ہو چکا ہے۔"

#: is-printing-error
msgid "The document cannot change while Printing or in Print Preview."
msgstr ""
"چھپائی یا چھپائی پیش نظارہ ہونے کے دوران دستاویز تبدیل نہیں کی جا سکتی۔"

#: denied-port-access
msgid "This address is restricted"
msgstr "یہ پتہ ممنوعہ ہے"

#: denied-port-access-error
msgid ""
"This address uses a network port which is normally used for purposes other "
"than Web browsing. The device has cancelled the request for your protection."
msgstr ""
"یہ پتہ ایسا نیٹ ورک پورٹ استعمال کرتا ہے جو عمومی طور پر ویب براوزنگ کے "
"علاوہ اور مقاصد کے لیے استعمال ہوتا ہے۔ آلہ نے آپ کی حفاظت کے لیے درخواست "
"منسوخ کر دی ہے۔"

#: proxy-resolve-failure
msgctxt "proxy-resolve-failure"
msgid "Proxy Server Error"
msgstr "پراکسی پیش کار نقص"

#: proxy-resolve-failure-error
msgid "The device is configured to use a proxy server that can’t be found."
msgstr "آلہ ایسا پراکسی پیش کار استعمال کرنے کے لیے تشکیل ہے جو مل نہیں رہا۔"

#: proxy-connect-failure
msgctxt "proxy-connect-failure"
msgid "Proxy Server Error"
msgstr "پراکسی پیش کار نقص"

#: proxy-connect-failure-error
msgid ""
"The device is configured to use a proxy server that is refusing connections."
msgstr ""
"آلہ ایسا پراکسی پیش کار استعمال کرنے کے لیے تشکیل ہے جو کنکشن نہیں کرنے دے "
"رہا۔"

#: content-encoding
msgctxt "content-encoding"
msgid "Can’t display page"
msgstr "صفحہ ظاہر نہیں کر سکتے"

#: content-encoding-error
msgid ""
"The device can’t display this page because a content encoding error occurred."
msgstr ""
"آلہ اس صفحے کو ظاہر نہیں کر سکتا کیونکہ ایک مواد کی ضابطہ بندی خرابی واقع "
"ہوئی ہے۔"

#: remote-xul
msgid "Page unsupported"
msgstr "صفحہ غیر معاون"

#: remote-xul-error
msgid ""
"This page can’t be displayed because it uses a technology that is not "
"supported on the device."
msgstr ""
"آلہ اس صفحے کو ظاہر نہیں کر سکتا کیونکہ یہ صفحہ وہ ٹیکنالوجی کا استعمال کرتا "
"ہے جس کی سہولت اس آلہ میں مہیا نہیں ہے۔"

#: phishing-blocked
msgid "Warning"
msgstr "تنبیہ"

#: phishing-blocked-error
msgid ""
"The website has been reported as a web forgery designed to trick users into "
"sharing personal or financial information."
msgstr ""
"یہ سائٹ نقلی سائٹ کے طور پر ریپورٹ کی گئی ہے جو صارفین سے دھوکے سے نجی یا "
"فائنینشل معلومات شیئر کروانے کے لیے بنائی گئی ہے۔"

#: malware-blocked
msgid "Malware attack blocked"
msgstr "مالویئر حملہ بلاک کردہ"

#: malware-blocked-error
msgid "The site at has been reported as an attack site and has been blocked."
msgstr ""
"سائٹ کو ایک حملے کی سائٹ کے طور پر رپورٹ کیا گیا ہے اور بلاک کردیا گیا ہے۔"

#: unsafe-content-type
msgid "Unsafe content"
msgstr "غیر محفوظ مواد"

#: unsafe-content-type-error
msgid "The page cannot be shown because it may not be safe to open."
msgstr ""
"صفحہ دکھایا نہیں جا سکتا کیونکہ ہو سکتا ہے یہ کھولنے کے لئے محفوظ نہ ہوں۔"

#: corrupted-content
msgctxt "corrupted-content"
msgid "Can’t display page"
msgstr "صفحے کو ظاہر نہیں کر سکتے ہیں"

#: corrupted-content-error
msgid ""
"The page cannot be shown because an error in the data transmission was "
"detected."
msgstr "صفحہ دکھایا نہیں جا سکتا کیونکہ کوائف کی منتقلی میں ایک نقص ملا۔"

#: unable-to-connect
msgctxt "unable-to-connect"
msgid "Unable to connect"
msgstr "جڑنے میں ناکام"

#: cancel
msgid "Cancel"
msgstr "منسوخ کریں"

#: close
msgid "Close"
msgstr "بند كریں"

#: try-again
msgid "Try again"
msgstr "پھر کوشش کریں"

#: check-settings
msgid "Check settings"
msgstr "سیٹکگیں کی پڑتال کریں"

#: tap-to-check-settings
msgid "Tap to check settings"
msgstr "سیٹکگیں کی پڑتال کرنے کے لئے کلک کرے"
