msgstr "Ześěgnjeńska zmólka"

#: install_error
msgid "Installation Error"
msgstr "Instalaciska zmólka"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "Ześěgnjenje njejo se serweroweje zmólki dla raźiło."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "Ześěgnjenje njejo se serweroweje zmólki dla raźiło."

#: err_too_big
msgid "File is too large to download."
msgstr "Dataja jo pśewjelika za ześěgnjenje."

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "Toś ten typ kupjonych medijow njepódpěra se na wašom telefonje."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "Ześěgnjenje jo se serweroweje zmólki dla raźiło."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "Ześěgnjenje jo se serweroweje zmólki dla raźiło."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "Ześěgnjenje jo se serweroweje zmólki dla raźiło."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "Ześěgnjony wobraz njedajo se na toś tom rěźe zwobrazniś."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "Ześěgnjona dataja njedajo se na toś tom rěźe wótgraś."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr ""
"Njejo dosć składowańskego ruma na wašom rěźe, aby tós ten spiw składowało."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "Spiw njedajo se składowaś, dokulaž składowańska kórrta njejo."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Spiw njedajo se składowaś, dokulaž składowańska kórta se južo wužywa. "
"Wótkajśo wój telefon a wopytajśo hyšći raz ześěgnuś."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Zmólka pśi składowanju medijoweje dataje."

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Zmólka pśi składowanju medijoweje dataje."

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr ""
"Spiw jo se ześěgnuł a zainstalěrował. Wužyjśo nałoženje Muzika, aby jen "
"wótgrał."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "Zwónjenje jo se ześěgnuło a zainstalěrowało."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "Slězynowy wobraz jo se ześěgnuł a zainstalěrował."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr "Cośo toś tu awdiodataju ako spiw abo ako zwónjenje instalěrowaś?"

