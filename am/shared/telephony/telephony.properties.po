msgstr "ቤት: {{ carrier }}"

#: phone_type_work_and_carrier
msgid "Work, {{ carrier }}"
msgstr "ስራ: {{ carrier }}"

#: phone_type_personal_and_carrier
msgid "Personal, {{ carrier }}"
msgstr "የ ግል: {{ carrier }}"

#: phone_type_fax_home_and_carrier
