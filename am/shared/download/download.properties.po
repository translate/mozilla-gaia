#: fileSize
msgid "{{size}} {{unit}}"
msgstr "{{size}} {{unit}}"

#: percentage
msgid "{{percentage}} %"
msgstr "{{percentage}} %"

#: partialResult
msgid "{{partial}} of {{total}}"
msgstr "{{partial}} ከ {{total}}"

#: summary
msgid "{{date}} - {{status}}"
msgstr "{{date}} - {{status}}"

#. # States
#: download-stopped
msgid "Stopped"
msgstr "ቆሟል"

#: download-failed
msgctxt "download-failed"
msgid "Download failed"
msgstr "ማውረድ አልተቻለም"

#: open_downloaded_file
msgid "Open"
msgstr "መክፈቻ"

#: share_downloaded_file
msgid "Share"
msgstr "ማካፈያ"

#: wallpaper_downloaded_file
msgid "Set as wallpaper"
msgstr "እንደ ግድግዳ ወረቀት ማሰናጃ"

#: ringtone_downloaded_file
msgid "Set as ringtone"
msgstr "እንደ ጥሪ ድምፅ ማሰናጃ"

#: cancel_downloaded_file
msgctxt "cancel_downloaded_file"
msgid "Cancel"
msgstr "መሰረዣ"
