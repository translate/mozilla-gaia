msgstr "{{name}} + încă {{n}}"

#: bt-status-paired[other]
msgctxt "bt-status-paired[other]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}} + încă {{n}}"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Atinge pentru conectare"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Se asociază cu dispozitivul…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Se așteaptă după celălalt dispozitiv…"

#: device-status-paired
msgid "Paired"
msgstr "Asociat"

#: device-status-connecting
msgid "Connecting…"
msgstr "Conectare…"

#: device-status-connected
msgid "Connected"
msgstr "Conectat"

#: device-status-connected-phone
msgid "Connected to phone audio"
msgstr "Conectat la sistemul audio al telefonului"

#: device-status-connected-media
msgid "Connected to media audio"
msgstr "Conectat la sistemul audio media"

#: device-status-connected-phone-media
msgid "Connected to phone/media audio"
msgstr "Conectat la sistemul audio al telefonului/media"

#: device-option-unpair
msgid "Unpair"
msgstr "Dezasociază"

#: device-option-confirm
msgid "Confirm"
msgstr "Confirmă"

#: device-option-unpair-confirmation
msgctxt "device-option-unpair-confirmation"
msgid "Confirmation"
msgstr "Confirmare"

#: device-option-unpair-device
msgid "Unpair device?"
msgstr "Dezasociezi dispozitivul?"

#: device-option-connect
msgid "Connect"
msgstr "Conectare"

#: device-option-disconnect
msgid "Disconnect"
msgstr "Deconectare"

#: search-for-device
msgid "Searching for devices…"
msgstr "Se caută dispozitive…"

#: search-device
msgid "Search for devices"
msgstr "Caută dispozitive"

#: rename-device
msgid "Rename my device"
msgstr "Redenumește-mi dispozitivul"

#: unnamed-device
msgid "Unknown device"
msgstr "Dispozitiv necunoscut"

#: change-phone-name
msgid "Change phone name"
msgstr "Schimbă numele telefonului"

#: change-phone-name-desc
msgid "Change the name of your device as it appears in your computer"
msgstr "Schimbă numele dispozitivului așa cum apare în calculator"

#: bluetooth-new-name
msgid "New name"
msgstr "Nume nou"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Nu se pot asocia dispozitive"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Nu se poate asocia cu dispozitivul. Verifică ca PIN-ul să fie corect."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Nu s-a putut asocia cu dispozitivul. Prea multe cereri."

#: error-connect-title
msgid "Unable to connect devices"
msgstr "Nu se poate conecta la dispozitive"

#: error-connect-msg
msgid ""
"Check that the device you’re trying to connect with is still in range and "
"has Bluetooth turned on."
msgstr ""
"Verifică ca dispozitivul cu care încerci să te conectezi este încă în rază "
"și are Bluetooth pornit."

#: unpair-title
msgid "Unpair a connected device?"
msgstr "Dezasociezi un dispozitiv conectat?"

#: unpair-msg
msgid ""
"You are currently connected to this device. Unpairing will also disconnect "
"from it."
msgstr ""
"În prezent ești conectat la acest dispozitiv. Dezasocierea va duce și la "
"deconectarea de acesta."

#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgctxt "confirmation"
msgid "Confirmation"
msgstr "Confirmare"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth este dezactivat"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Dorești să pornești Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Renunță"

#: turn-on
msgid "Turn On"
msgstr "Pornește"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Transferul de fișiere prin Bluetooth a eșuat"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Transferul de fișiere prin Bluetooth a eșuat. Verifică dacă setările "
"Bluetooth sunt corecte."

#: ok
msgid "OK"
msgstr "Ok"

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Asociază"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Confirmă dispozitivele pentru asociere"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Pentru a asocia cu {{device}}, asigură-te că ambele dispozitivele au "
"următorul PIN afișat:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Pentru a asocia cu {{device}}, introdu PIN-ul pe dispozitiv."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Pentru a asocia cu {{device}}, introdu cheia de acces în dispozitiv."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Cerere de asociere Bluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} dorește să se asocieze cu acest telefon. Pentru a accepta, "
"asigură-te că ambele dispozitive au următorul PIN afișat:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} dorește să se asocieze cu acest telefon. Pentru a accepta, "
"introdu PIN-ul pe dispozitiv."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} dorește să se asocieze cu acest telefon. Pentru a accepta, "
"introdu cheia de acces în dispozitiv."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Cerere Bluetooth de la"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Cerere de asociere Bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Cerere de asociere Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Cererea de asociere Bluetooth a expirat. Încearcă din nou asocierea."
