#: manage-title-2
msgid "Manage Tones"
msgstr "Gestionează tonuri"

#: actions-share-ringtone
msgid "Share ringtone"
msgstr "Partajează tonul de apel"

#: actions-delete-ringtone
msgid "Delete ringtone"
msgstr "Șterge tonul de apel"

#: actions-share-alerttone
msgid "Share alert"
msgstr "Partajează alerta"

#: actions-delete-alerttone
msgid "Delete alert"
msgstr "Șterge alerta"

#: actions-cancel
msgctxt "actions-cancel"
msgid "Cancel"
msgstr "Renunță"

#: delete-title-ringtone
msgid "Delete Ringtone"
msgstr "Șterge tonul de apel"

#: delete-title-alerttone
msgid "Delete Alert"
msgstr "Șterge alerta"

#: delete-message
msgid "Are you sure you want to delete \"<bdi>{{tone}}</bdi>\"?"
msgstr "Sigur dorești să ștergi „<bdi>{{tone}}</bdi>”?"

#: delete-message-default-ringtone
msgid ""
"\"<bdi>{{tone}}</bdi>\" is the current default ringtone.<br><br>Are you sure "
"you want to delete it?"
msgstr ""
"„<bdi>{{tone}}</bdi>” e tonul de apel actual implicit.<br><br>Sigur dorești "
"să-l ștergi?"

#: delete-message-default-alerttone
msgid ""
"\"<bdi>{{tone}}</bdi>\" is the current default alert.<br><br>Are you sure "
"you want to delete it?"
msgstr ""
"„<bdi>{{tone}}</bdi>” e alerta actuală implicită.<br><br>Sigur dorești să o "
"ștergi?"

#: delete-cancel
msgctxt "delete-cancel"
msgid "Cancel"
msgstr "Renunță"

#: delete-confirm
msgid "Delete"
msgstr "Șterge"

#: deleted-ringtone
msgid "Ringtone deleted."
msgstr "Ton de apel șters."

#: deleted-alerttone
msgid "Alert deleted."
msgstr "Alertă ștearsă."

#: created-tone
msgid "New ringtone created."
msgstr "Ton de apel nou creat."

#: set-default-tone
msgid "Ringtone set as default."
msgstr "Ton de apel setat ca implicit."

#: sd-card-subtitle
msgid "SD Card"
msgstr "Card SD"

#. ##### Ringtone Picking #####
#: pick-title
msgid "Select Sound"
msgstr "Selectează sunetul"

#: pick-set
msgid "Set"
msgstr "Setează"

#. ##### List Section Headers #####
#: section-title-builtin-ringtone
msgid "{{ringtoneSectionBrandName}} Ringtones"
msgstr "Tonuri de apel {{ringtoneSectionBrandName}}"

#: section-title-builtin-alerttone
msgid "{{ringtoneSectionBrandName}} Alerts"
msgstr "Alerte {{ringtoneSectionBrandName}}"

#: section-title-custom-ringtone
msgid "My Ringtones"
msgstr "Tonurile mele"

#: section-title-custom-alerttone
msgid "My Alerts"
msgstr "Alertele mele"

#. ##### Ringtone Sharing (creation) #####
#: create-title
msgid "Create Ringtone"
msgstr "Creează ton de apel"

#: create-save
msgid "Save"
msgstr "Salvează"

#: create-cancel
msgctxt "create-cancel"
msgid "Cancel"
msgstr "Renunță"

#: play-error-title
msgid "Corrupted File"
msgstr "Fișier corupt"

#: play-error-desc
msgid "The file is corrupted. It cannot be used to create a ringtone."
msgstr ""
"Fișierul este corupt. Nu poate fi folosit pentru a crea un ton de apel."

#: save-error-title
msgid "Ringtone Not Created"
msgstr "Ton de apel necreat"

#: save-error-desc
msgid "There was an issue creating the ringtone. Please try again."
msgstr "A apărut o problemă la crearea tonului de apel. Te rugăm să reîncerci."

#: default-label
msgid "Use as default ringtone"
msgstr "Folosește ca ton implicit"

#: ok
msgid "OK"
msgstr "Ok"

#: ringtone-untitled
msgid "Untitled"
msgstr "Fără titlu"

