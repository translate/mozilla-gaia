msgid "{{name}}, +{{n}} more"
msgstr "{{name}} eta {{n}} gehiago"

#: bt-status-paired[many]
msgctxt "bt-status-paired[many]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}} eta {{n}} gehiago"

#: bt-status-paired[other]
msgctxt "bt-status-paired[other]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}} eta {{n}} gehiago"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Sakatu konektatzeko"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Gailuarekin parekatzen…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Beste gailuari itxoiten…"

#: device-status-paired
msgid "Paired"
msgstr "Parekatuta"

#: device-status-connecting
msgid "Connecting…"
msgstr "Konektatzen…"

#: device-status-connected
msgid "Connected"
msgstr "Konektatuta"

#: device-status-connected-phone
msgid "Connected to phone audio"
msgstr "telefonoaren audiora konektatuta"

#: device-status-connected-media
msgid "Connected to media audio"
msgstr "Multimediaren audiora konektatuta"

#: device-status-connected-phone-media
msgid "Connected to phone/media audio"
msgstr "Telefonoaren/multimediaren audiora konektatuta"

#: device-option-unpair
msgid "Unpair"
msgstr "Desparekatu"

#: device-option-confirm
msgid "Confirm"
msgstr "Berretsi"

#: device-option-unpair-confirmation
msgctxt "device-option-unpair-confirmation"
msgid "Confirmation"
msgstr "Berrespena"

#: device-option-unpair-device
msgid "Unpair device?"
msgstr "Desparekatu gailua?"

#: device-option-connect
msgid "Connect"
msgstr "Konektatu"

#: device-option-disconnect
msgid "Disconnect"
msgstr "Deskonektatu"

#: search-for-device
msgid "Searching for devices…"
msgstr "Gailuak bilatzen…"

#: search-device
msgid "Search for devices"
msgstr "Bilatu gailuak"

#: rename-device
msgid "Rename my device"
msgstr "Aldatu nire gailuaren izena"

#: unnamed-device
msgid "Unknown device"
msgstr "Gailu ezezaguna"

#: change-btdevice-name
msgid "Change device name"
msgstr "Aldatu gailuaren izena"

#: change-btdevice-name-desc
msgid "This name will appear on your other Bluetooth devices"
msgstr "Izen hau zure beste Bluetooth gailuetan agertuko da"

#: bluetooth-new-name
msgid "New name"
msgstr "Izen berria"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ezin dira gailuak parekatu"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ezin izan da gailuarekin parekatu. Egiaztatu PINa zuzena dela."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ezin izan da gailuarekin parekatu. Eskari gehiegi."

#: error-connect-title
msgid "Unable to connect devices"
msgstr "Ezin dira gailuak konektatu"

#: error-connect-msg
msgid ""
"Check that the device you’re trying to connect with is still in range and "
"has Bluetooth turned on."
msgstr ""
"Egiaztatu konektatzen saiatzen ari zaren gailua oraindik barrutian dagoela "
"eta Bluetooth-a piztuta daukala."

#: unpair-title
msgid "Unpair a connected device?"
msgstr "Desparekatu konektatutako gailua?"

#: unpair-msg
msgid ""
"You are currently connected to this device. Unpairing will also disconnect "
"from it."
msgstr ""
"Orain gailu honekin konektatuta zaude. Desparekatzeak harekin deskonektatzea "
"eragingo du."

#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgctxt "confirmation"
msgid "Confirmation"
msgstr "Berrespena"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth-a desgaituta dago"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Bluetooth-a piztu nahi duzu?"

#: cancel
msgid "Cancel"
msgstr "Utzi"

#: turn-on
msgid "Turn On"
msgstr "Piztu"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Huts egin du Bluetooth bidezko fitxategi-transferentziak"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Huts egin du Bluetooth bidezko fitxategi-transferentziak. Egiaztatu "
"Bluetooth ezarpenak zuzenak direla."

#: ok
msgid "OK"
msgstr "Ados"

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Parekatzea"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Berretsi gailuak parekatzea"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"{{device}} gailuarekin parekatzeko, ziurtatu bi gailuek ondorengo PINa "
"bistan dutela:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "{{device}} gailuarekin parekatzeko, sartu gailuko PINa."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "{{device}} gailuarekin parekatzeko, sartu gailuko pasagakoa."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Bluetooth parekatze-eskaera"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, ziurtatu bi "
"gailuek honako PINa bistan dutela:"

#: passive-pair-pincode
msgctxt "passive-pair-pincode"
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu PINa "
"gailuan."

#: passive-pair-enterpincode
msgctxt "passive-pair-enterpincode"
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu PINa "
"gailuan."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu "
"pasagakoa gailuan."

#: passive-pair-displaypasskey
msgid ""
"{{device}} would like to pair with this phone. To accept, input the passkey "
"and then press Return or Enter on the device."
msgstr ""
"{{device}} gailuak telefono honekin parekatu nahi du. Onartzeko, sartu "
"pasagakoa eta ondoren sakatu Sartu tekla gailuan."

#: passive-pair-consent
msgid "{{device}} would like to pair with this phone."
msgstr "{{device}} gailuak telefono honekin parekatu nahi du."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Bluetooth eskaera hemendik"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Bluetooth parekatze-eskaera"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Bluetooth parekatze-eskaera"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Bluetooth parekatze-eskaera iraungi da. Saiatu berriro parekatzen."

