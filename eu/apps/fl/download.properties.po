#: download_error
msgid "Download Error"
msgstr "Deskarga-errorea"

#: install_error
msgid "Installation Error"
msgstr "Instalazio-errorea"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "Deskargak huts egin du zerbitzariaren errore bat dela eta."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "Deskargak huts egin du zerbitzariaren errore bat dela eta."

#: err_too_big
msgid "File is too large to download."
msgstr "Fitxategia handiegia da deskargatzeko."

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "Zure gailuak ez du erositako multimedia mota hau onartzen."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "Deskargak huts egin du zerbitzariaren errore bat dela eta."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "Deskargak huts egin du zerbitzariaren errore bat dela eta."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "Deskargak huts egin du zerbitzariaren errore bat dela eta."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "Ezin da deskargatutako irudia bistaratu gailu honetan."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "Ezin da deskargatutako fitxategia erreproduzitu gailu honetan."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr "Ez dago abesti hau biltegiratzeko leku libre nahikorik zure gailuan."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "Ezin da abestia gorde memoria-txartelik ez dagoelako."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Ezin da abestia gorde memoria-txartela erabiltzen ari delako. Desentxufatu "
"telefonoa eta saiatu berriro deskargatzen."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Errorea multimedia-fitxategia gordetzean."

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Errorea multimedia-fitxategia gordetzean."

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr ""
"Abestia deskargatuta eta instalatuta. Erreproduzitzeko, erabili Musika "
"aplikazioa."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "Txirrin-tonua deskargatuta eta instalatuta."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "Horma-papera deskargatuta eta instalatuta."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr "Audio-fitxategia abesti edo txirrin-tonu gisa instalatu nahi duzu?"

#: ringtone-response
msgid "Ringtone"
msgstr "Txirrin-tonua"

#: song-response
msgid "Song"
msgstr "Abestia"

#: download-confirmation-title-song
msgid "Download song?"
msgstr "Deskargatu abestia?"

#: download-confirmation-title-ringtone
msgid "Download ringtone?"
msgstr "Deskargatu txirrin-tonua?"

#: download-confirmation-title-wallpaper
msgid "Download wallpaper?"
msgstr "Deskargatu horma-papera?"

#: downloading-song
msgid "Downloading song…"
msgstr "Abestia deskargatzen…"

#: downloading-ringtone
msgid "Downloading ringtone…"
msgstr "Txirrin-tonua deskargatzen…"

#: downloading-wallpaper
msgid "Downloading wallpaper…"
msgstr "Horma-papera deskargatzen…"

#: locked-content-advisory
msgid ""
"{{vendor}} has locked this media file so that it cannot be shared with "
"others or transferred to a new device."
msgstr ""
"{{vendor}} hornitzaileak multimedia-fitxategia blokeatu du ezin dadin beste "
"inorekin partekatu edo gailu berri batera transferitu."

#. # This version is like the above, but used when we don't know the name of
#. # the vendor.
#: locked-content-advisory-generic
msgid ""
"The seller has locked this media file so that it cannot be shared with "
"others or transferred to a new device."
msgstr ""
"Saltzaileak multimedia-fitxategia blokeatu du ezin dadin beste inorekin "
"partekatu edo gailu berri batera transferitu."

