# EN: 01 PM. AF: 01:00
#. # Date formatting details (http://pubs.opengroup.org/onlinepubs/007908799/xsh/strftime.html)
#: hour-format
msgid "%I %p"
msgstr "%H"

#: hour-allday
msgid "All Day"
msgstr "Heeldag"

#: current-time
msgid "%I:%M"
msgstr "%H:%M"

# EN: Jan 2010, AF: Jan 2010
#. # Default header used in month view / week view
#: multi-month-view-header-format
msgid "%b %Y"
msgstr "%b %Y"

# EN: Jan 1, Monday. AF: Maandag 1 Jan
#. # Header format used for in the day view.
#: day-view-header-format
msgid "%b %e, %A"
msgstr "%A %e %b"

# EN: Monday, Jan 1. AF: Maandag 1 Jan
#. # Header used on month view before the event list (selected day date)
#: months-day-view-header-format
msgid "%A, %b %e"
msgstr "%A %e %b"

# EN: Monday, January 01, 2010. AF: Monday 1 January 2010
#. # Date format in the event detail view.
#: longDateFormat
msgid "%A, %B %d, %Y"
msgstr "%A %e %B %Y"

