#. # Prompt that appears when a user tries to delete an app/bookmark.
#: delete-title
msgid "Delete {{name}}"
msgstr "{{name}} കളയുക"

#: delete-body
msgid "{{name}} and all of its data will be deleted."
msgstr "{{name}} ഉം അതിന്റെ എല്ലാ ഡാറ്റയും കളയും."

#: delete
msgid "Delete"
msgstr "കളയുക"

#: cancel
msgid "Cancel"
msgstr "റദ്ദാക്കു"

#. # Context menu options
#: homescreen-settings
msgid "Homescreen Settings"
msgstr ""

#: change-wallpaper
msgid "Change Wallpaper"
msgstr "പശ്ചാത്തലചിത്രം മാറ്റുക"

#: edit-mode
msgid "Edit"
msgstr "തിരുത്തുക"

#: edit-done
msgid "Done"
msgstr "തീര്‍ന്നു"

#. # Prompt that appears when a user tries to stop a download.
#: stop-download-title
msgid "Stop {{name}}"
msgstr "{{name}} നിര്‍ത്തുക"

#: stop-download-body
msgid "Do you want to stop the download in progress?"
