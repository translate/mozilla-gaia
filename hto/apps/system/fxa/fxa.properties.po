#. # 'spec' references below refer to the spec attached to bug 960130
#. # this is a real error screen (7.2 in spec)
#: fxa-connection-error-title
msgctxt "fxa-connection-error-title"
msgid "Unable to Connect"
msgstr "Nítaoinide"

#: fxa-connection-error-message
msgid "You can create an account or sign in from Settings."
msgstr "fakara fɨno ímei Ajuste dɨnodo ó jófo bí"

#. # this is a real error screen (5.4 in spec)
#: fxa-reset-password-error-title
msgid "Reset Password"
msgstr "Fakára ráa ábɨdo fɨno"

#: fxa-reset-password-error-message
msgid "You can reset your password from Settings."
msgstr "Ajuste dɨnodo ó fákara abɨdo jóne."

#. # this is a real error screen (19.4 in spec)
#: fxa-coppa-failure-error-title
msgid "Cannot Create Account"
msgstr "Fɨnónide  makara"

#: fxa-coppa-failure-error-message3
msgid ""
"You must meet certain age requirements to create a Firefox Account. <a>Learn "
"more</a>"
msgstr ""

#. # this is a real error screen (13.2 and 17.2 in spec)
#: fxa-offline-error-title
msgctxt "fxa-offline-error-title"
msgid "Unable to Connect"
msgstr "Nítaoinide"

#: fxa-offline-error-message
msgid "Please connect to a network from Settings."
msgstr "Ajustes fóikui íyano nítaoi"

#. # this is a real error screen (3.2 in spec)
#: fxa-invalid-email-title
msgid "Error"
msgstr "Fɨgòñede"

#: fxa-invalid-email-message
msgid "Please enter a valid email."
msgstr "Ñúe úiraɨma kúeno."

#. # this is a real error screen (5.2 / 11.1 in spec)
#: fxa-invalid-password-title
msgid "Invalid Password"
msgstr "Fakára ráa dái íeñede"

#: fxa-invalid-password-message
msgid "Please check your password and try again."
msgstr "Fakára ráa ábɨdo ñúe éroi."

#. # this is the fallback error
#: fxa-unknown-error-title
msgctxt "fxa-unknown-error-title"
msgid "Unable to Connect"
msgstr "Ñúitanide"

#: fxa-unknown-error-message
msgid "System unavailable, try again soon."
msgstr "Sistema íñede, daruimo ábɨdo éroiri."

