msgstr "Cancelar"

#: download_error
msgid "Download Error"
msgstr "Erro ao descarregar"

#: install_error
msgid "Installation Error"
msgstr "Erro na Instalação"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "A descarga falhou devido a um erro do servidor."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "A descarga falhou devido a um erro do servidor."

#: err_too_big
msgid "File is too large to download."
msgstr "O ficheiro é demasiado grande para ser descarregado."

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "Este tipo multimédia não é suportado pelo seu dispositivo."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "A descarga falhou devido a um erro do servidor."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "A descarga falhou devido a um erro do servidor."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "A descarga falhou devido a um erro do servidor."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "A imagem descarregada não pode ser exibida neste dispositivo."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "O ficheiro descarregado não pode ser reproduzido neste dispositivo."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr ""
"Não existe espaço suficiente no seu dispositivo para guardar esta música."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr ""
"Não foi possível guardar a música porque não existe nenhum cartão de memória."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Não foi possível guardar a música porque o cartão de memória está a ser "
"utilizado. Desligue o cabo do equipamento e volte a tentar descarregar."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Erro ao guardar o ficheiro multimédia."

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Erro ao guardar o ficheiro multimédia."

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr ""
"Música descarregada e instalada. Utilize a aplicação Música para a "
"reproduzir."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "Toque descarregado e instalado."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "Papel de parede descarregado e instalado."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr ""
"Gostaria de instalar este ficheiro áudio como música ou como tom de toque?"

#: ringtone-response
msgid "Ringtone"
msgstr "Tom de toque"

#: song-response
msgid "Song"
msgstr "Música"

