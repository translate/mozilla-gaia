#. # Set & Enter Password
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-create-password-title
msgid "Create a Password"
msgstr "Sos Finnde"

#: fxa-enter-password-title
msgid "Enter Your Password"
msgstr "Naatnu Fiinde Maa"

#: fxa-hello-user2
msgid "Hello, <a>{{email}}</a>"
msgstr ""

#: fxa-password-refresh
msgid "Please enter your Firefox Account password to continue."
msgstr "Tiiɗno naatnu finnde Konte Firefox maa ngam jokkude."

#: fxa-password.placeholder
msgid "Password"
msgstr "Finnde"

#: fxa-forgot-your-password
msgid "Forgot your password?"
msgstr "A yejjitii finnde maa?"

#: fxa-show-password
msgid "Show password"
msgstr "Hollu finnde"

#: fxa-password-length-requirement
msgid "Your password must be at least 8 characters long."
msgstr "Finnde ma ena foti waɗde alkule 8 so famɗii."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signin success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-success
msgid "Success!"
msgstr "Moƴƴii!"

#: fxa-ready-to-go
msgid "Your Firefox Account is ready to go! "
msgstr "Konte Firefox maa hebiima huutorteede! "

#: fxa-use-features
msgid ""
"Now you can use features like Find My Device and apps like the Firefox "
"Marketplace."
msgstr ""
"Jooni aɗa waawi huutortaade fannuuji wano Yiytu Kaɓirgol Am kañum e jaaɓɗe "
"bayɗe hono Firefox Marketplace."

#: fxa-access-manage-account-settings
msgid "In the future, you can access and manage your account from Settings."
msgstr "Ko fayi arde, aɗa waawi naatde e toppitaade konte maa to Teelte too."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signup success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-confirm-account
msgid "Confirm Your Account"
msgstr "Teeŋtin Konte Maa"

#: fxa-will-send-email2
msgid "We will send an email to: <strong>{{email}}</strong>"
msgstr ""

#: fxa-signup-click-verification-link-in-email
msgid ""
"Next time you check your email, click the verification link to confirm your "
"Firefox Account."
msgstr ""
"So a ƴeewtiima iimeeluuji maa; dobo e jokkol ƴeewtorgol ngol ngam teeŋtinde "
"Konte Firefox maa."

