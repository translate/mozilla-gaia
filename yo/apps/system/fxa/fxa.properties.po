#: firefox-accounts-title
msgid "Firefox Accounts"
msgstr "Àkọọ́lẹ̀ Firefox"

#: fxa-next
msgid "Next"
msgstr "Tókàn"

#: fxa-back
msgid "Back"
msgstr "Ẹ̀yìn"

#: fxa-done
msgid "Done"
msgstr "Ti ṣe tán"

#: fxa-ok
msgid "OK"
msgstr "Ó DÁRA"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Errors
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # 'spec' references below refer to the spec attached to bug 960130
#. # this is a real error screen (7.2 in spec)
#: fxa-connection-error-title
msgctxt "fxa-connection-error-title"
msgid "Unable to Connect"
msgstr "Kòle sopọ̀"

#: fxa-connection-error-message
msgid "You can create an account or sign in from Settings."
msgstr "O lè ṣẹ̀dá àkọọ́lẹ̀ tàbí wọlé láti ààtò."

#. # this is a real error screen (5.4 in spec)
#: fxa-reset-password-error-title
msgid "Reset Password"
msgstr "Ṣàtúntò Ọ̀rọ̀ àṣirí"

#: fxa-reset-password-error-message
msgid "You can reset your password from Settings."
msgstr "O lè ṣàtúntò ọ̀rọ̀ aṣínà rẹ láti ààtò."

#. # this is a real error screen (19.4 in spec)
#: fxa-coppa-failure-error-title
msgid "Cannot Create Account"
msgstr "Kòle Ṣẹ̀dá Ákọọ́lẹ̀"

#: fxa-coppa-failure-error-message2
msgid ""
"You must meet certain age requirements to create a Firefox Account. <a id="
"\"coppa-link\" href=\"#\">Learn more</a>"
msgstr ""

#. # this is a real error screen (13.2 and 17.2 in spec)
#: fxa-offline-error-title
msgctxt "fxa-offline-error-title"
msgid "Unable to Connect"
msgstr "Kòle sopọ̀"

#: fxa-offline-error-message
msgid "Please connect to a network from Settings."
msgstr "Jọ̀wọ́ sopọ̀ mọ́ nẹ́tíwọkì láti ààtọ̀."

#. # this is a real error screen (3.2 in spec)
#: fxa-invalid-email-title
msgid "Error"
msgstr "Àṣìṣe"

#: fxa-invalid-email-message
msgid "Please enter a valid email."
msgstr "Jọ̀wọ́ ṣàmúwọlé í-meélì àìfẹsẹ̀múlẹ̀."

#. # this is a real error screen (5.2 / 11.1 in spec)
#: fxa-invalid-password-title
msgid "Invalid Password"
msgstr "Òrọ̀ aṣínà Àìfẹsẹ̀múlẹ̀"

#: fxa-invalid-password-message
msgid "Please check your password and try again."
msgstr "Jọ̀wọ́ yẹ aṣínà rè wò kí o sì gbìyànjú lẹ́ẹ̀kan si."

#. # this is the fallback error
#: fxa-unknown-error-title
msgctxt "fxa-unknown-error-title"
msgid "Unable to Connect"
msgstr "Kò lè Sopọ̀mọ́"

#: fxa-unknown-error-message
msgid "System unavailable, try again soon."
msgstr "Kòsí ètò, gbìyànjú lẹ́ẹ̀kan si."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Interstitial screen
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-connecting
msgid "Connecting"
msgstr "Ń so pọ̀mọ́"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Enter Email
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-enter-email-input.placeholder
msgid "Enter your email"
msgstr "Ṣàtẹ̀wọlé í-meèlí rẹ"

#: fxa-use-email-to-sign-in
msgid "Use your email address to create an account or sign in."
msgstr "Lo àdírẹ́ẹ̀sì í-meèlí láti ṣẹ̀dá àkọọ́lẹ̀ tàbí wọlé."

#: fxa-notice2
#, fuzzy
msgid ""
"By proceeding, I agree to the <a>{{tos}}</a> and <a>{{pn}}</a> of Firefox "
"cloud services."
msgstr ""
"Nípa tìtẹ̀siwajú, Mo gbà sí {{tos}} àti {{pn}} tii iṣẹ́ àsopọ̀ ẹlẹ̀ka Firefox."

#. # LOCALIZATION NOTE(fxa-tos): This string corresponds to the {{tos}} placeholder
#. # in fxa-notice.
#: fxa-tos
msgid "Terms of Service"
msgstr "Àwọn òfin Iṣẹ́"

#. # LOCALIZATION NOTE(fxa-pn): This string corresponds to the {{pn}} placeholder
#. # in fxa-notice.
#: fxa-pn
msgid "Privacy Notice"
msgstr "Àkíyèsí Ìkọ̀kọ̀"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Set & Enter Password
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-create-password-title
msgid "Create a Password"
msgstr "Ṣẹ̀dá Ọ̀rọ̀ aṣínà"

#: fxa-enter-password-title
msgid "Enter Your Password"
msgstr "Ṣàwọle pẹ̀lú Ọ̀rọ̀ Aṣínà rẹ"

#: fxa-hello-user2
msgid "Hello, <a>{{email}}</a>"
msgstr ""

#: fxa-password-refresh
msgid "Please enter your Firefox Account password to continue."
msgstr "Jọ̀wọ́ wọlé sí ọ̀rọ̀ aṣìná Àkọọ́lẹ̀ Firefox rẹ láti tẹ̀siwájú."

#: fxa-password.placeholder
msgid "Password"
msgstr "Ọ̀rọ̀ aṣínà"

#: fxa-forgot-your-password
msgid "Forgot your password?"
msgstr "O gbàgbé ọ̀rọ̀ aṣínà rẹ?"

#: fxa-show-password
msgid "Show password"
msgstr "Ṣàfihàn ọ̀rọ̀ aṣínà"

#: fxa-password-length-requirement
msgid "Your password must be at least 8 characters long."
msgstr "Ọ̀rọ̀ aṣínà rẹ gbọ́dọ̀ ni ó kéré àwọn ẹyọ ọ̀rọ̀ tí ó gùn ní 8."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signin success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-success
msgid "Success!"
msgstr "Àṣeyọrí!"

#: fxa-ready-to-go
msgid "Your Firefox Account is ready to go! "
msgstr "Àkọọ́lẹ̀ Firefox rẹ ṣetán láti ṣiṣẹ́!"

#: fxa-use-features
msgid ""
"Now you can use features like Find My Device and apps like the Firefox "
"Marketplace."
msgstr ""
"Báyì o lè lo àwọn àbùdá bíi Wá Ohun elò Mi àti ìṣàfilọ́lẹ̀ bíi Ibiọjà Firefox."

#: fxa-access-manage-account-settings
msgid "In the future, you can access and manage your account from Settings."
msgstr "Ní ọjọ́ iwájú, o lè wọlé kí o ṣàbojutó àkọọ́lẹ̀ rẹ láti ààtò."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signup success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-confirm-account
msgid "Confirm Your Account"
msgstr "Jẹ́rì Àkọọ́lẹ̀ Rẹ"

#: fxa-will-send-email2
msgid "We will send an email to: <strong>{{email}}</strong>"
msgstr ""

#: fxa-signup-click-verification-link-in-email
msgid ""
"Next time you check your email, click the verification link to confirm your "
"Firefox Account."
msgstr ""
"Nígba míìràn tí o báyẹ í-meélì rẹ wò, tẹ ìtọ́kasí ìṣàmúdájú láti jẹ́rì Àkọọ́lẹ̀ "
"Firefox rẹ."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # COPPA
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-age-verification
msgid "Age Verification"
msgstr "Ìṣàmúdájú Ọjọ́ orí"

#: fxa-age-earlier
msgid "{{year}} or earlier"
msgstr ""

#: fxa-year-of-birth
msgid "Year of Birth"
msgstr "Ọdún Ìbí"

