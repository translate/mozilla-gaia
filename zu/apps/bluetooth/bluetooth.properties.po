#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "I-Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Isiqinisekiso"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "I-Bluetooth icishiwe"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Uyafuna ukuvula i-Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Khansela"

#: turn-on
msgid "Turn On"
msgstr "Vula"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Ukuthunyelwa kwefayela nge-Bluetooth akuphumelelanga"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Ukuthunyelwa kwefayela nge-Bluetooth akuphumelelanga. Hlolisisa ukuthi "
"amasethingi e-Bluetooth asesimeni."

#: ok
msgid "OK"
msgstr "Kulungile"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Amadivayisi ahambisanayo"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Amadivayisi akule ndawo"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Thinta kancane ukuze uxhumeke"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Isazama ukuxhumana nedivayisi…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Ilindele enye idivayisi…"

#: device-status-paired
msgid "Paired"
msgstr "Asexhumene"

#: device-status-connecting
msgid "Connecting…"
msgstr "Isaxhuma…"

#: device-status-connected
msgid "Connected"
msgstr "Ixhumekile"

#: search-for-device
msgid "Searching for devices…"
msgstr "Isacinga amadivayisi…"

#: search-device
msgid "Search for devices"
msgstr "Cinga amadivayisi"

#: unnamed-device
msgid "Unknown device"
msgstr "Idivayisi engaziwa"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Amadivayisi awavumi ukuhambisana"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ayikwazi ukuhambisana nedivayisi. Hlola ukuthi i-PIN ilungile na."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ayikwazi ukuhambisana nedivayisi. Kunezicelo eziningi kakhulu."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Matanisa"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Qinisekisa amadivayisi azomataniswa"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Ukuze umatanise {{device}}, qiniseka iukuthi womabili amadivayisi "
"njengamanje aveze i-PIN elandelayo:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Ukuze umatanise {{device}}, faka i-PIN kudivayisi."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Ukuze umatanise ne-{{device}}, faka i-passkey kudivayisi."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Isicelo sokumatanisa nge-Bluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"I-{{device}} ingathanda ukumataniswa nale foni. Ukuze wamukele lesi sicelo, "
"qinisekisa ukuthi womabili amadivayisi njengamanje aveze le PIN elandelayo:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"I-{{device}} ingathanda ukumataniswa nale foni. Ukuze wamukele lesi sicelo, "
"faka i-PIN kudivayisi."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"I-{{device}} ingathanda ukumataniswa nale foni. Ukuze wamukele, faka i-"
"passkey kudivayisi."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Isicelo Se-Bluetooth Esivela"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Isicelo sokumataniswa nge-Bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Isicelo Sokumataniswa Nge-Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr ""
"Isicelo sokumataniswa nge-Bluetooth siphelelwe yisikhathi. Phinda uzame "
"ukumatanisa futhi."
