#: network-error-in-app
msgid "No access to the Internet right now."
msgstr "We wokomiku žadyn přistup k internetej njeje."

#: network-error-launching
msgid "{{name}} requires an Internet connection."
msgstr "{{name}} trjeba internetny zwisk."

#: server-not-found
msgid "Server not found"
msgstr "Serwer njeje so namakał"

#: server-not-found-error
msgid "Can’t find the server at {{name}}."
msgstr "Serwer njeda so na {{name}} namakać."

#: file-not-found
msgid "File not found"
msgstr "Dataja njeje so namakała"

#: file-not-found-error
msgid "The device can’t display this page because the file cannot be found."
msgstr "Grat njemóže tutu stronu zwobraznić, dokelž dataja njeje so namakała."

#: connection-failed
msgctxt "connection-failed"
msgid "Unable to connect"
msgstr "Zwisk njemóžno"

#: connection-failed-error
msgctxt "connection-failed-error"
msgid "A network error occurred while trying to reach the site."
msgstr "Syćowy zmylk je wustupił, mjeztym zo sće spytał, so k sydłu dóstać."

#: invalid-connection
msgctxt "invalid-connection"
msgid "Unable to connect"
msgstr "Zwisk njemóžno"

#: invalid-connection-error
msgctxt "invalid-connection-error"
msgid "A network error occurred while trying to reach the site."
msgstr "Syćowy zmylk je wustupił, mjeztym zo sće spytał, so k sydłu dóstać."

#: malformed-uri
msgid "URL not valid"
msgstr "URL płaćiwy njeje"

#: malformed-uri-error
msgid "The URL entered is not valid and cannot be loaded."
msgstr "Podaty URL njeje płaćiwy a njeda so začitać."

#: redirect-loop
msgctxt "redirect-loop"
msgid "Can’t display page"
msgstr "Strona njeda so zwobraznić"

#: redirect-loop-error
msgid ""
"The device can’t display this page because the page is not redirecting "
"properly. Try this page again at a later time."
msgstr ""
"Grat njemóže tutu stronu zwobraznić, dokelž strona porjadnje dale "
"njesposrědkuje. Spytajće pozdźišo hišće raz."

#: not-cached
msgid "Page expired"
msgstr "Strona je spadnjena"

#: not-cached-error
msgid ""
"As a security precaution, this page cannot be displayed by the device "
"because it has expired."
msgstr ""
"Jako wěstotna naprawa njeda so tuta strona přez grat zwobraznić, dokelž je "
"spadnjena."

#: is-printing-error
msgid "The document cannot change while Printing or in Print Preview."
msgstr "Dokument njeda so při ćišćenju abo w ćišćerskim přehledźe změnić."

#: denied-port-access
msgid "This address is restricted"
msgstr "Tuta adresa je zamjezowana"

#: denied-port-access-error
msgid ""
"This address uses a network port which is normally used for purposes other "
"than Web browsing. The device has cancelled the request for your protection."
msgstr ""
"Tuta adresa wužiwa syćowy port, kotryž so normalnje za druhe zaměry hač "
"webpřehladowanje wužiwa. Grat je naprašowanje za waš škit přetorhnył."

#: proxy-resolve-failure
msgctxt "proxy-resolve-failure"
msgid "Proxy Server Error"
msgstr "Zmylk proksy-serwera"

#: proxy-resolve-failure-error
msgid "The device is configured to use a proxy server that can’t be found."
msgstr ""
"Grat je so skonfigurował, zo by proksy-serwer wužiwał, kotryž njeda so "
"namakać."

#: proxy-connect-failure
msgctxt "proxy-connect-failure"
msgid "Proxy Server Error"
msgstr "Zmylk proksy-serwera"

#: proxy-connect-failure-error
msgid ""
"The device is configured to use a proxy server that is refusing connections."
msgstr ""
"Grat je so skonfigurował, zo by proksy-serwer wužiwał, kotryž zwiski "
"wotpokazuje."

#: content-encoding
msgctxt "content-encoding"
msgid "Can’t display page"
msgstr "Strona njeda so zwobraznić"

#: content-encoding-error
msgid ""
"The device can’t display this page because a content encoding error occurred."
msgstr ""
"Grat njemóže tutu stronu zwobraznić, dokelž kodowanski zmylk wobsaha je "
"wustupił."

#: remote-xul
msgid "Page unsupported"
msgstr "Strona so njepodpěruje"

#: remote-xul-error
msgid ""
"This page can’t be displayed because it uses a technology that is not "
"supported on the device."
msgstr ""
"Tuta strona njeda so zwobraznić, dokelž technologiju wužiwa, kotraž so na "
"graće njepodpěruje."

#: phishing-blocked
msgid "Warning"
msgstr "Warnowanje"

#: phishing-blocked-error
msgid ""
"The website has been reported as a web forgery designed to trick users into "
"sharing personal or financial information."
msgstr ""
"Websydło je so jako webfalšowanka zdźěliło, kotraž je za to myslena, "
"wužiwarjow zawjedować, wosobinske abo financielne daty přeradźić."

#: malware-blocked
msgid "Malware attack blocked"
msgstr "Nadpad złowólneje softwary je so zablokował"

#: malware-blocked-error
msgid "The site at has been reported as an attack site and has been blocked."
msgstr "Sydło je so jako nadpadowace sydło zdźěliło a je so zablokowało."

#: unsafe-content-type
msgid "Unsafe content"
msgstr "Njewěsty wobsah"

#: unsafe-content-type-error
msgid "The page cannot be shown because it may not be safe to open."
msgstr "Strona njeda so pokazać, dokelž njeda so ewentuelnje wěsće wočinić."

#: corrupted-content
msgctxt "corrupted-content"
msgid "Can’t display page"
msgstr "Strona njeda so zwobraznić"

#: corrupted-content-error
msgid ""
"The page cannot be shown because an error in the data transmission was "
"detected."
msgstr ""
"Strona njeda so pokazać, dokelž je so zmylk w datowym přenjesenju wotkrył."

#: unable-to-connect
msgctxt "unable-to-connect"
msgid "Unable to connect"
msgstr "Zwisk njemóžno"

#: cancel
msgid "Cancel"
msgstr "Přetorhnyć"

#: close
msgid "Close"
msgstr "Začinić"

#: try-again
msgid "Try again"
msgstr "Hišće raz spytać"

#: check-settings
msgid "Check settings"
msgstr "Nastajenja přepruwować"

#: tap-to-check-settings
msgid "Tap to check settings"
msgstr "Podótkńće so, zo byšće nastajenja přepruwował"
