msgctxt "failed_download_right_button"
msgid "Try again"
msgstr "Mēģināt vēlreiz"

#: stop_download_title
msgid "Stop download"
msgstr "Apstādināt lejupielādi"

#: stop_download_message
msgid "Are you sure you want to stop the download?"
msgstr "Vai esat drošs ka vēlaties apstādināt lejupielādi?"

#: stop_download_left_button
msgid "No"
msgstr "Jā"

#: stop_download_right_button
msgid "Yes"
msgstr "Nē"

