msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, + još {{n}}"

#: bt-status-paired[two]
msgctxt "bt-status-paired[two]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, + još {{n}}"

#: bt-status-paired[few]
msgctxt "bt-status-paired[few]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, + još {{n}}"

#: bt-status-paired[many]
msgctxt "bt-status-paired[many]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, + još {{n}}"

#: bt-status-paired[other]
msgctxt "bt-status-paired[other]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, + još {{n}}"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Pritisnite za povezivanje"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Uparivanje uređaja…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Čekanje drugog uređaja…"

#: device-status-paired
msgid "Paired"
msgstr "Uparen"

#: device-status-connecting
msgid "Connecting…"
msgstr "Povezivanje…"

#: device-status-connected
msgid "Connected"
msgstr "Povezan"

#: device-status-connected-phone
msgid "Connected to phone audio"
msgstr "Povezan na zvuk telefona"

#: device-status-connected-media
msgid "Connected to media audio"
msgstr "Povezan na zvuk multimedije"

#: device-status-connected-phone-media
msgid "Connected to phone/media audio"
msgstr "Povezan na zvuk telefona/multimedije"

#: device-option-unpair
msgid "Unpair"
msgstr "Raspari"

#: device-option-confirm
msgid "Confirm"
msgstr "Potvrdi"

#: device-option-unpair-confirmation
msgctxt "device-option-unpair-confirmation"
msgid "Confirmation"
msgstr "Potvrda"

#: device-option-unpair-device
msgid "Unpair device?"
msgstr "Raspari uređaj?"

#: device-option-connect
msgid "Connect"
msgstr "Poveži"

#: device-option-disconnect
msgid "Disconnect"
msgstr "Prekini vezu"

#: search-for-device
msgid "Searching for devices…"
msgstr "Tražim uređaje…"

#: search-device
msgid "Search for devices"
msgstr "Traži uređaje"

#: rename-device
msgid "Rename my device"
msgstr "Promijeni naziv mog uređaja"

#: unnamed-device
msgid "Unknown device"
msgstr "Nepoznat uređaj"

#: change-btdevice-name
msgid "Change device name"
msgstr "Promijeni naziv uređaja"

#: change-btdevice-name-desc
msgid "This name will appear on your other Bluetooth devices"
msgstr "Ovaj naziv će se prikazivati na vašim drugim Bluetooth uređajima"

#: bluetooth-new-name
msgid "New name"
msgstr "Novi naziv"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Uparivanje nije moguće"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Uređaje nije moguće upariti. Provjerite da li je PIN ispravan."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Uređaje nije moguće upariti. Previše zahtjeva."

#: error-connect-title
msgid "Unable to connect devices"
msgstr "Nije moguće povezati uređaje"

#: error-connect-msg
msgid ""
"Check that the device you’re trying to connect with is still in range and "
"has Bluetooth turned on."
msgstr ""
"Provjerite da li je uređaj na koji se povezujete u dometu i da li je na "
"njemu uključen Bluetooth."

#: unpair-title
msgid "Unpair a connected device?"
msgstr "Raspariti povezani uređaj?"

#: unpair-msg
msgid ""
"You are currently connected to this device. Unpairing will also disconnect "
"from it."
msgstr ""
"Trenutno ste povezani sa ovim uređajem. Rasparivanjem će ta veza biti "
"prekinuta."

#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgctxt "confirmation"
msgid "Confirmation"
msgstr "Potvrda"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth je isključen"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Da li želite uključiti Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Otkaži"

#: turn-on
msgid "Turn On"
msgstr "Uključi"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Prijenos fajla putem Bluetootha nije uspio"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Prijenos fajla putem Bluetootha nije uspio. Provjerite da li su ispravne "
"Bluetooth postavke."

#: ok
msgid "OK"
msgstr "OK"

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Upari"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Potvrdite uređaje za uparivanje"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Za uparivanje sa {{device}}, provjerite da li je na oba uređaja trenutno "
"prikazan sljedeći PIN:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Da uparite sa {{device}}, upišite PIN na uređaju."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Da uparite sa {{device}}, upišite šifru na uređaju."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Zahtjev za uparivanje uređaja"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} se želi upariti sa vašim telefonom. Da biste prihvatili, "
"provjerite da li oba uređaja trenutno imaju prikazan sljedeći PIN:"

#: passive-pair-pincode
msgctxt "passive-pair-pincode"
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} se želi upariti sa ovim telefonom. Da prihvatite, unesite PIN sa "
"uređaja."

#: passive-pair-enterpincode
msgctxt "passive-pair-enterpincode"
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} se želi upariti sa ovim telefonom. Da prihvatite, unesite PIN sa "
"uređaja."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} se želi upariti sa ovim telefonom. Da biste prihvatili, upišite "
"šifru na uređaju."

#: passive-pair-displaypasskey
msgid ""
"{{device}} would like to pair with this phone. To accept, input the passkey "
"and then press Return or Enter on the device."
msgstr ""
"{{device}} se želi upariti sa ovim telefonom. Da prihvatite, unesite šifru i "
"pritisnite Return ili Enter na uređaju."

#: passive-pair-consent
msgid "{{device}} would like to pair with this phone."
msgstr "{{device}} se želi upariti sa ovim telefonom."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Bluetooth zahtjev od"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Zahtjev za uparivanje uređaja preko Bluetootha"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Zahtjev za uparivanje uređaja preko Bluetootha"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Bluetooth zahtjev za uparivanje je istekao. Pokušajte ponovo."

