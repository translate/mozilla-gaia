#. # Prompt that appears when a user tries to stop a download.
#: gaia-grid-stop-download-title
msgid "Stop {{name}}"
msgstr "Juk {{name}}"

#: gaia-grid-stop-download-body
msgid "Do you want to stop the download in progress?"
msgstr "Imito juko gam ma tye ka wot ni?"

#: gaia-grid-stop-download-action
msgid "Stop"
msgstr "Juki"

#. # Prompt that appears when a user tries to resume a download.
#: gaia-grid-resume-download-title
msgid "Restart Download"
msgstr "Nwo cako gamo"

#: gaia-grid-resume-download-body
msgid "Do you want to download {{name}}?"
msgstr "Imito gamo {{name}}?"

#: gaia-grid-resume-download-action
msgid "Download"
msgstr "Gam"

#. # Prompt that appears when removing an app
#: gaia-grid-unrecoverable-error-title
msgid "Remove Application"
msgstr "Kwany purugram"

#: gaia-grid-unrecoverable-error-body
msgid ""
"Cannot install application due to an unrecoverable error. Please try "
"downloading again. The application will be removed."
msgstr ""
"Pe twero keto purugram pi bal ma pe twero yubo. Tim ber i tem gamo doki. Ki "
"bikwanyo purugram ne woko."

#: gaia-grid-unrecoverable-error-action
msgid "OK"
msgstr "EYO"

#: gaia-grid-cancel
msgid "Cancel"
msgstr "Juki"
