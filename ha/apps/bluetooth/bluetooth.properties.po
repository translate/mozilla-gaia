msgstr "{{name}}, +{{n}} ƙari"

#: bt-status-paired[other]
msgctxt "bt-status-paired[other]"
msgid "{{name}}, +{{n}} more"
msgstr "{{name}}, +{{n}} ƙari"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Danna domin haɗawa"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Ana tagwaitawa da na'ura…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Ya na jiran wata na'urar…"

#: device-status-paired
msgid "Paired"
msgstr "An tagwaita"

#: device-status-connecting
msgid "Connecting…"
msgstr "Haɗawa…"

#: device-status-connected
msgid "Connected"
msgstr "An haɗa"

#: device-status-connected-phone
msgid "Connected to phone audio"
msgstr "An haɗa da na saurare na waya"

#: device-status-connected-media
msgid "Connected to media audio"
msgstr "An haɗa da na saurare na kiɗa"

#: device-status-connected-phone-media
msgid "Connected to phone/media audio"
msgstr "An haɗa da waya/na saurare na kiɗa"

#: device-option-unpair
msgid "Unpair"
msgstr "Raba tagwaitawa"

#: device-option-confirm
msgid "Confirm"
msgstr "Tabbatar"

#: device-option-unpair-confirmation
msgctxt "device-option-unpair-confirmation"
msgid "Confirmation"
msgstr "Tabbatarwa"

#: device-option-unpair-device
msgid "Unpair device?"
msgstr "Raba tagwaita na'ura?"

#: device-option-connect
msgid "Connect"
msgstr "Haɗa"

#: device-option-disconnect
msgid "Disconnect"
msgstr "Cire"

#: search-for-device
msgid "Searching for devices…"
msgstr "Ya na neman na'urori…"

#: search-device
msgid "Search for devices"
msgstr "Nemi na'urori"

#: rename-device
msgid "Rename my device"
msgstr "Sakewa na'urata suna"

#: unnamed-device
msgid "Unknown device"
msgstr "Na'ura da ba'a sani ba"

#: change-phone-name
msgid "Change phone name"
msgstr "Canja sunan waya"

#: change-phone-name-desc
msgid "Change the name of your device as it appears in your computer"
msgstr "Canja sunan na'urarka kamar yadda ta fito a kwamfutarka"

#: bluetooth-new-name
msgid "New name"
msgstr "Sabon suna"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ya kasa tagwaita na'urori"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ya kasa tagwaitawa da na'urar. Duba cewa PIN daidai ne."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ya kasa tagwaita da na'urar. Akwai bukatu masu yawa."

#: error-connect-title
msgid "Unable to connect devices"
msgstr "Ya gaza haɗa na'urori"

#: error-connect-msg
msgid ""
"Check that the device you’re trying to connect with is still in range and "
"has Bluetooth turned on."
msgstr ""
"Duba cewa na'urar da ka ke son haɗawa da ita ta na kusa kuma Bluetooth na ta "
"ya na kunne."

#: unpair-title
msgid "Unpair a connected device?"
msgstr "A raba tagwaitawa da na'urar da aka haɗa?"

#: unpair-msg
msgid ""
"You are currently connected to this device. Unpairing will also disconnect "
"from it."
msgstr ""
"A yanzu an haɗa ka da wannan na'urar. Cire tagwaitawa kuma zai cire haɗi da "
"ita."

#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgctxt "confirmation"
msgid "Confirmation"
msgstr "Tabbatarwa"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "An dakatar da bluetooth"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Kana so ka kunna Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Soke"

#: turn-on
msgid "Turn On"
msgstr "Kunna"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Ya kasa canja fayil ta bluetooth"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Ya kasa canja fayil ta Bluetooth. Laluba cewa saitunan Bluetooth dai dai "
"suke."

#: ok
msgid "OK"
msgstr "OK"

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Tagwai"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Tabbatar da na'urorin da za'a tagwaita"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Domin tagwaita da {{device}}, ka tabbatar cewa na'urorin guda biyu na da "
"waɗannan PIN ɗin da aka nuna:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Domin tagwaita da {{device}}, shigar da PIN ɗin da ke kan na'urar."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Domin tagwaita da {{device}}, shigar da mabuɗin da ke kan na'urar."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Bukatar tagwaita bluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} na so ta tagwaita da wannan wayar. Domin karɓa, ka tabbatar "
"na'urorin biyu a yanzu su na da waɗannan PIN ɗin da aka nuna:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} na so ta tagwaita wa wannan wayar. Domin karɓa, shigar da PIN ɗin "
"da ke kan na'urar."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} na so ta tagwaita da wannan wayar. Domin karɓa, shigar da mabuɗin "
"da ke kan na'urar."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Bukatar Bluetooth Daga"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Bukatar tagwaita bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Bukatar Tagwaita Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Bukatar tagwaita bluetooth ta ƙare. Sake jarraba tagwaitawa."
