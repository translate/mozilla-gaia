#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Uthibitisho"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth imelemazwa"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Je, unataka kuwasha Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Ghairi"

#: turn-on
msgid "Turn On"
msgstr "Washa"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Kupeperushwa kwa faili kwa kutumia bluetooth kumeshindwa"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Buetooth imeshindwa kupeperusha faili. Kagua mipangilio ya bluetooth iko "
"sahihi."

#: ok
msgid "OK"
msgstr "SAWA"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Vifaa vilivyooanishwa"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Vifaa katika eneo"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Gusa ili kuunganisha"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Inaunganisha na kifaa…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Inangojea kifaa kingine…"

#: device-status-paired
msgid "Paired"
msgstr "Kimeunganishwa"

#: device-status-connecting
msgid "Connecting…"
msgstr "Inaunganisha…"

#: device-status-connected
msgid "Connected"
msgstr "Imeunganishwa"

#: search-for-device
msgid "Searching for devices…"
msgstr "Inatafuta vifaa…"

#: search-device
msgid "Search for devices"
msgstr "Tafuta vifaa"

#: unnamed-device
msgid "Unknown device"
msgstr "Kifaa kisichojulikana"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Imeshindwa kuonisha vifaa"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Imeshindwa kuungana na kifaa. Angalia ya kwamba PIN ni sahihi."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Imeshindwa kuonishwa na kifaa. Maombi mengi sana."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Jozi"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Thibitisha vifaa kuwa jozi"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Ili kuoanisha na {{device}}, hakikisha ya kwamba vifaa vyote kwa sasa "
"vinaonyesha PIN ifuatayo:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Ili kuoanisha na {{device}}, ingiza PIN kwenye kifaa."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Ili kuoanisha na {{device}}, ingiza ufunguo wa pitisho kwenye kifaa."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Ombi la kuwa jozi kupitia Bluetooh"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{device}} ingependa kuoanisha na simu hii. Ili kukubali, hakikisha ya "
"kwamba vifaa vyote kwa sasa vinaonyesha PIN ifuatayo:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{device}} ingependa kuoanisha na simu hii. Ili kukubali, ingiza PIN kwenye "
"kifaa."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{device}} ingependa kuoanisha na simu hii. Ili kukubali, ingiza ufunguo wa "
"pitisho kwenye kifaa."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Ombi la Bluetooth Kutoka"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Ombi la kuwa jozi kupitia Bluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Ombi la Kuwa Jozi la Bluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Ombi la kuwa jozi la Bluetooth limekwisha muda. Jaribu tena kuwa jozi."

