#: percentage
msgid "{{percentage}} %"
msgstr "{{percentage}} %"

#: partialResult
msgid "{{partial}} of {{total}}"
msgstr "{{partial}} de {{total}}"

#: summary
msgid "{{date}} - {{status}}"
msgstr "{{date}} - {{status}}"

#. # States
#: download-stopped
msgid "Stopped"
msgstr "Air stad"

#: download-failed
msgctxt "download-failed"
msgid "Download failed"
msgstr "Dh’fhàillig an luchdadh a-nuas"

#: open_downloaded_file
msgid "Open"
msgstr "Fosgail"

#: share_downloaded_file
msgid "Share"
msgstr "Co-roinn"

#: wallpaper_downloaded_file
msgid "Set as wallpaper"
msgstr "Suidhich mar phàipear-balla"

#: ringtone_downloaded_file
msgid "Set as ringtone"
msgstr "Suidhich mar sheirm"

#: cancel_downloaded_file
msgctxt "cancel_downloaded_file"
msgid "Cancel"
msgstr "Sguir dheth"

