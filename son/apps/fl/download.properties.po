msgstr "Ayyo"

#: download-button
msgid "Download"
msgstr "Zumandi"

#: install-button
msgid "Install"
msgstr "Sinji"

#: cancel-button
msgid "Cancel"
msgstr "Naŋ"

#: download_error
msgid "Download Error"
msgstr "Zumandi firka"

#: install_error
msgid "Installation Error"
msgstr "Sinjiyan firka"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "Zumandiyanoo dunbu feršikaw firka sabbu se."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "Zumandiyanoo dunbu feršikaw firka sabbu se."

#: err_too_big
msgid "File is too large to download."
msgstr "Tuku ga hansa ka beeri zumandiyan se."

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr "Hooray goyjinay dumoo woo ši goy nda war talfonoo."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "Zumandiyanoo dunbu feršikaw firka sabbu se."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "Zumandiyanoo dunbu feršikaw firka sabbu se."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "Zumandiyanoo dunbu feršikaw firka sabbu se."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "Bii zunbantaa ši hin ka cebandi jinaa woo ga."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "Tuku zunbantaa ši hin ka hoorandi jinaa woo ga."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr "Jišidogoo ši wasa war jinaa ga ka doonoo woo gaabu."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "Ši hin ka doonoo gaabu zama lakkal katta kul šii."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Ši hin ka doonoo gaabu zama lakkal kattaa goo goy ra. Talfonoo kaa kuuraŋ "
"nda ma zumandi koyne."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Firla hooray tuku gaabuyan ra."

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Firla hooray tuku gaabuyan ra."

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr "Doonoo zumandi nd'a sinjandi. Miiziki porogaram k'a hẽenandi."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "Talfon hẽeni zumandi nd'a sinjandi."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "Cetakaddasu zumandi nd'a sinjandi."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr ""
"War ga ba ka jinde tukoo woo zumandi k'a sinji sanda dooni wala talfon hẽeni?"

#: ringtone-response
msgid "Ringtone"
msgstr "Talfon hẽeni"

#: song-response
msgid "Song"
msgstr "Dooni"

