#: voiceMail
msgid "Voicemail"
msgstr "Umyalezo welizwi"

#~ msgid "To make a call you need to disable airplane mode in settings."
#~ msgstr "Ukuze utsale umnxeba kufuneka uyenze ingasebenzi imo yenqwelo-moya."

#~ msgctxt "callAirplaneModeBtnOk"
#~ msgid "OK"
#~ msgstr "Kulungile"

#~ msgid "Airplane mode activated"
#~ msgstr "Imo yenqwelo-moya iyasebenza"

#~ msgid "Number is busy"
#~ msgstr "Inombolo ixakekile"

#~ msgid "The number is currently busy. Please try again later."
#~ msgstr "Inombolo ixakekile okwangoku. Sicela uphinde uzame kamva."

#~ msgid "FDN active"
#~ msgstr "I-FDN iyasebenza"

#~ msgid ""
#~ "FDN (Fixed Dialing Numbers) is activated and the following number is not "
#~ "in your FDN list: {{ number }}"
#~ msgstr ""
#~ "I-FDN (Fixed Dialing Numbers) yenziwe yasebenza kwaye le nombolo "
#~ "ilandelayo ayikho kuluhlu lwakho lwe-FDN: {{ number }}"

#~ msgid "Emergency number"
#~ msgstr "Inombolo yongxamiseko"

#~ msgid "To make a call, the phone must be connected to a network."
#~ msgstr "Ukuze utsale umnxeba, ifowuni ifanele inxibelelane ne-nethiweki."

#~ msgid "Emergency calls are not allowed by the network."
#~ msgstr "Iminxeba yongxamiseko ayivunyelwa yinethiweki."

#~ msgctxt "emergencyDialogBtnOk"
#~ msgid "OK"
#~ msgstr "Kulungile"

#~ msgid "No network connection"
#~ msgstr "Akukho lunxibelelwano lwenethiweki"

#~ msgid "Invalid phone number"
#~ msgstr "Inombolo yefowuni engesabenziyo"

#~ msgid "The phone number you are calling is not valid."
#~ msgstr "Inombolo yefowuni oyitsalelayo ayisebenzi."

#~ msgid "Unable to make a phone call now"
#~ msgstr "Ayikwazi ukutsala umnxeba ngoku"

#~ msgid ""
#~ "Cannot make a call. If you are already dialing, please hang up first."
#~ msgstr ""
#~ "Ayikwazi ukutsala umnxeba. Ukuba sele utsala umnxeba, sicela uqale "
#~ "uyibeke phantsi."

#~ msgid "Other connection in use"
#~ msgstr "Olunye unxibelelwano olusetyenziswayo"

#~ msgid ""
#~ "You have an active call on another connection. You can only use one "
#~ "connection at a time."
#~ msgstr ""
#~ "Unomnxeba olindileyo kolunye unxibelelwano. Ungasebenzisa unxibelelwano "
#~ "olunye ngexesha."

#~ msgid ""
#~ "This number is blocked as it is not in your list of authorized numbers."
#~ msgstr ""
#~ "Le nombolo ivaliwe njengoko ingekho kuluhlu lweenombolo zakho "
#~ "ezigunyazisiweyo."
